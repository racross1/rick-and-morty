<h2> Hi New User! </h1>
<%= form_with model: @user  do |f|%>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
    
    <%= f.submit %>
<% end %>